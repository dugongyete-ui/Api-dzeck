{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/api/chat",
        "destination": "https://api-dzeck--mio7wxa.replit.app/chat",
        "type": 307
      },
      {
        "source": "/v1/chat/completions",
        "destination": "https://api-dzeck--mio7wxa.replit.app/completions",
        "type": 307
      },
      {
        "source": "/stream",
        "destination": "https://api-dzeck--mio7wxa.replit.app/stream",
        "type": 307
      },
      {
        "source": "/health",
        "destination": "https://api-dzeck--mio7wxa.replit.app/health",
        "type": 307
      },
      {
        "source": "/ping",
        "destination": "https://api-dzeck--mio7wxa.replit.app/ping",
        "type": 307
      },
      {
        "source": "/generatetoken",
        "destination": "https://api-dzeck--mio7wxa.replit.app/generatetoken",
        "type": 307
      },
      {
        "source": "/api/apikeys",
        "destination": "https://api-dzeck--mio7wxa.replit.app/apikeys",
        "type": 307
      },
      {
        "source": "/api/apikeys/**",
        "destination": "https://api-dzeck--mio7wxa.replit.app/:splat",
        "type": 307
      },
      {
        "source": "/api/auth/**",
        "destination": "https://api-dzeck--mio7wxa.replit.app/:splat",
        "type": 307
      },
      {
        "source": "/api/chat-history",
        "destination": "https://api-dzeck--mio7wxa.replit.app/chat-history",
        "type": 307
      },
      {
        "source": "/api/conversations",
        "destination": "https://api-dzeck--mio7wxa.replit.app/conversations",
        "type": 307
      },
      {
        "source": "/api/conversations/**",
        "destination": "https://api-dzeck--mio7wxa.replit.app/:splat",
        "type": 307
      },
      {
        "source": "/api/chat-settings",
        "destination": "https://api-dzeck--mio7wxa.replit.app/chat-settings",
        "type": 307
      },
      {
        "source": "/api/model-info",
        "destination": "https://api-dzeck--mio7wxa.replit.app/model-info",
        "type": 307
      },
      {
        "source": "/api/settings/**",
        "destination": "https://api-dzeck--mio7wxa.replit.app/:splat",
        "type": 307
      },
      {
        "source": "/api/providers-data",
        "destination": "https://api-dzeck--mio7wxa.replit.app/providers-data",
        "type": 307
      },
      {
        "source": "/api/users/**",
        "destination": "https://api-dzeck--mio7wxa.replit.app/:splat",
        "type": 307
      }
    ],
    "rewrites": [
      {
        "source": "/settings",
        "destination": "/settings.html"
      },
      {
        "source": "/models",
        "destination": "/models.html"
      },
      {
        "source": "/test-api",
        "destination": "/test-api.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ]
  }
}
